<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTDMapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="signUp">

   <insert id="signUpStudent" parameterType="dto.StudentVO">
         insert into student values (null, #{id}, #{name}, #{pw}, #{email}, #{phone})       
    </insert>

    <insert id="signUpProfessor" parameterType="dto.ProfessorVO">
        insert into professor values (#{professorid}, #{pro_name}, #{pr_id}, #{pr_pw})            
    </insert>

</mapper>